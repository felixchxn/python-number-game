import random

def guess_number():
    number = random.randint(1, 100)
    attempts = 0

    while True:
        guess = int(input("Guess a number between 1 and 100: "))
        attempts += 1

        if guess == number:
            print("Congratulations! You guessed the correct number in", attempts, "attempts.")
            break
        elif guess < number:
            print("Too low. Try again.")
        else:
            print("Too high. Try again.")

guess_number()
